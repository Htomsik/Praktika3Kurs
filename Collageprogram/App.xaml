<Application
    x:Class="CollageProgram.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:CollageProgram"
    xmlns:viewmodels="clr-namespace:CollageProgram.ViewModels"
    xmlns:views="clr-namespace:CollageProgram.Views"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\ViewResources\ViewSwitch.xaml" />
                <ResourceDictionary Source="Resources\DataTemplate\MenuDataTemplate.xaml" />
                <ResourceDictionary Source="Resources\Themes\DefaulTheme.xaml" />
                <ResourceDictionary Source="Resources\Styles\ToolTipStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
